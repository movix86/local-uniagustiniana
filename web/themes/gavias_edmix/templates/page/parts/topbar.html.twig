<div class="topbar">
    <div class="container">
        <div class="row">
            <div class="topbar-left col-xs-12 col-sm-12 col-md-4 text-center">
                <div class="social-list">
                    {% if custom_social_link.facebook %}
                        <a href="{{custom_social_link.facebook}}" target="_blank">
                            <i class="fa fa-facebook"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.twitter %}
                        <a href="{{custom_social_link.twitter}}" target="_blank">
                            <i class="fa fa-twitter-square"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.skype %}
                        <a href="{{custom_social_link.skype}}" target="_blank">
                            <i class="fa fa-skype"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.instagram %}
                        <a href="{{custom_social_link.instagram}}" target="_blank">
                            <i class="fa fa-instagram"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.dribbble %}
                        <a href="{{custom_social_link.dribbble}}" target="_blank">
                            <i class="fa fa-dribbble"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.linkedin %}
                        <a href="{{custom_social_link.linkedin}}" target="_blank">
                            <i class="fa fa-linkedin-square"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.pinterest %}
                        <a href="{{custom_social_link.pinterest}}" target="_blank">
                            <i class="fa fa-pinterest"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.google %}
                        <a href="{{custom_social_link.google}}" target="_blank">
                            <i class="fa fa-google-plus-square"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.youtube %}
                        <a href="{{custom_social_link.youtube}}" target="_blank">
                            <i class="fa fa-youtube-square"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.vimeo %}
                        <a href="{{custom_social_link.vimeo}}" target="_blank">
                            <i class="fa fa-vimeo-square"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.tumblr %}
                        <a href="{{custom_social_link.tumblr}}" target="_blank">
                            <i class="fa fa-tumblr-square"></i>
                        </a>
                    {% endif %}
                    <!--PRUEBA-->
                    {% if custom_social_link.mail %}
                        <a href="{{custom_social_link.mail}}" target="_blank">
                            <i class="fa fa-envelope-square"></i>
                        </a>
                    {% endif %}
                    {% if custom_social_link.siga %}
                        <a href="{{custom_social_link.siga}}" target="_blank">
                            SIGA
                        </a>
                    {% endif %}
                    <!--PRUEBA-->
                </div>
            </div>
            <div class="topbar-right col-sm-8 col-xs-3">
                <div class="right-topbar-h">
                    <div class="topbar-close hidden-lg hidden-md">
                        <i class="fa fa-times"></i>
                    </div>
                    {{ drupal_block('MenuRol')}}
                    {# {% if custom_account == '' %}
                        <ul class="gva_topbar_menu">
                            <li>
                                <a href="{{ login_link }}">{{'Login'|t}}</a>
                            </li>
                            <li>
                                <a href="{{ register_link }}">{{'Register'|t}}</a>
                            </li>
                        </ul>
                    {% else %}
                        <ul class="gva_topbar_menu login">
                            <li>{{ custom_account|raw }}</li>
                        </ul>
                    {% endif %} #}
                </div>
                
                {# <div id="menu-bartop" class="menu-bartop hidden-lg hidden-md">
                    <span class="one"></span>
                    <span class="two"></span>
                    <span class="three"></span>
                </div> #}
            </div>
        </div>
    </div>
</div>
