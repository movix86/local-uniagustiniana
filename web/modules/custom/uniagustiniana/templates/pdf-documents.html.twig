{#
/**
 * @file
 * Template for the order receipt.
 *
 * Available variables:
 * - pdfs: array pdfs
 * @ingroup themeable
 */
#}

{% for key, url in pdfs %}
    <div id="portalpdf" class="portalpdf">
        <object data="{{ url }}" type="application/pdf" width="100%" height="100%"></object>
    </div>
{% endfor %}


